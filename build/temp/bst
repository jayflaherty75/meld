#!/bin/bash

mkdir -p ./build/d/console
echo "#!/bin/bash" > ./build/d/console/bst
echo "" >> ./build/d/console/bst
echo "./build/m/bst" >> ./build/d/console/bst

chmod +x ./build/d/console/bst

mkdir -p ./build/d/fault
echo "#!/bin/bash" > ./build/d/fault/bst
echo "" >> ./build/d/fault/bst
echo "./build/m/bst" >> ./build/d/fault/bst

chmod +x ./build/d/fault/bst

mkdir -p ./build/d/error-handling
echo "#!/bin/bash" > ./build/d/error-handling/bst
echo "" >> ./build/d/error-handling/bst
echo "./build/m/bst" >> ./build/d/error-handling/bst

chmod +x ./build/d/error-handling/bst

mkdir -p ./build/d/tester
echo "#!/bin/bash" > ./build/d/tester/bst
echo "" >> ./build/d/tester/bst
echo "./build/m/bst" >> ./build/d/tester/bst

chmod +x ./build/d/tester/bst

mkdir -p ./build/d/iterator
echo "#!/bin/bash" > ./build/d/iterator/bst
echo "" >> ./build/d/iterator/bst
echo "./build/m/bst" >> ./build/d/iterator/bst

chmod +x ./build/d/iterator/bst


