#!/bin/bash

mkdir -p ./build/d/console
echo "#!/bin/bash" > ./build/d/console/default
echo "" >> ./build/d/console/default
echo "./build/m/default" >> ./build/d/console/default

chmod +x ./build/d/console/default

mkdir -p ./build/d/fault
echo "#!/bin/bash" > ./build/d/fault/default
echo "" >> ./build/d/fault/default
echo "./build/m/default" >> ./build/d/fault/default

chmod +x ./build/d/fault/default

mkdir -p ./build/d/error-handling
echo "#!/bin/bash" > ./build/d/error-handling/default
echo "" >> ./build/d/error-handling/default
echo "./build/m/default" >> ./build/d/error-handling/default

chmod +x ./build/d/error-handling/default

mkdir -p ./build/d/tester
echo "#!/bin/bash" > ./build/d/tester/default
echo "" >> ./build/d/tester/default
echo "./build/m/default" >> ./build/d/tester/default

chmod +x ./build/d/tester/default


