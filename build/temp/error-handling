#!/bin/bash

mkdir -p ./build/d/console
echo "#!/bin/bash" > ./build/d/console/error-handling
echo "" >> ./build/d/console/error-handling
echo "./build/m/error-handling" >> ./build/d/console/error-handling

chmod +x ./build/d/console/error-handling

mkdir -p ./build/d/fault
echo "#!/bin/bash" > ./build/d/fault/error-handling
echo "" >> ./build/d/fault/error-handling
echo "./build/m/error-handling" >> ./build/d/fault/error-handling

chmod +x ./build/d/fault/error-handling


