<?xml version="1.0" encoding="UTF-8"?>
<module name="console-c">
  <requires module='fault' version='0.1.0' />
  <requires module='sys' version='0.1.0' />
  <requires module='tester' version='0.1.0' />
  <version>0.1.0</version>
  <function name='startup'>
    <returns type='short' modifier='value' />
    <description>Application main routine.</description>
  </function>
  <function name='shutdown'>
    <returns type='short' modifier='value' />
    <description>Application main routine.</description>
  </function>
  <function name='test'>
    <param name='describeFn' type='any ptr' modifier='value' const='false' />
    <returns type='short' modifier='value' />
    <description>Standard test runner for modules.</description>
  </function>
  <function name='logMessage'>
    <param name='message' type='zstring' modifier='pointer' const='false' />
    <description>Log a message to the console.</description>
  </function>
  <function name='logWarning'>
    <param name='id' type='zstring' modifier='pointer' const='false' />
    <param name='message' type='zstring' modifier='pointer' const='false' />
    <param name='source' type='zstring' modifier='pointer' const='false' />
    <param name='lineNum' type='integer' modifier='value' const='false' />
    <description>Log a warning to the console.</description>
  </function>
  <function name='logError'>
    <param name='id' type='zstring' modifier='pointer' const='false' />
    <param name='message' type='zstring' modifier='pointer' const='false' />
    <param name='source' type='zstring' modifier='pointer' const='false' />
    <param name='lineNum' type='integer' modifier='value' const='false' />
    <description>Log an error to the console.</description>
  </function>
  <function name='logSuccess'>
    <param name='id' type='zstring' modifier='pointer' const='false' />
    <param name='message' type='zstring' modifier='pointer' const='false' />
    <param name='source' type='zstring' modifier='pointer' const='false' />
    <param name='lineNum' type='integer' modifier='value' const='false' />
    <description>Log a success message to the console.</description>
  </function>
  <function name='_time'>
    <param name='buffer' type='zstring' modifier='pointer' const='false' />
    <returns type='zstring' modifier='pointer'>
      <description>Simply returns the buffer pointer for chaining</description>
    </returns>
    <private />
  </function>
  <namespace>ConsoleC</namespace>
</module>
