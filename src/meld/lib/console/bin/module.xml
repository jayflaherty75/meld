<?xml version="1.0" encoding="UTF-8"?>
<module name="console">
  <requires module='fault_v0.1.0' version='1' />
  <requires module='error-handling_v0.1.0' version='1' />
  <requires module='sys_v0.1.0' version='1' />
  <version>0.1.0</version>
  <function name='startup'>
    <returns type='short' modifier='value' />
    <description>Application main routine.</description>
  </function>
  <function name='shutdown'>
    <returns type='short' modifier='value' />
    <description>Application main routine.</description>
  </function>
  <function name='logMessage'>
    <param name='message' type='zstring' modifier='reference' const='false' />
    <description>Log a message to the console.</description>
  </function>
  <function name='logWarning'>
    <param name='id' type='zstring' modifier='reference' const='false' />
    <param name='message' type='zstring' modifier='reference' const='false' />
    <param name='source' type='zstring' modifier='reference' const='false' />
    <param name='lineNum' type='integer' modifier='value' const='false' />
    <description>Log a warning to the console.</description>
  </function>
  <function name='logError'>
    <param name='id' type='zstring' modifier='reference' const='false' />
    <param name='message' type='zstring' modifier='reference' const='false' />
    <param name='source' type='zstring' modifier='reference' const='false' />
    <param name='lineNum' type='integer' modifier='value' const='false' />
    <description>Log an error to the console.</description>
  </function>
  <function name='logSuccess'>
    <param name='id' type='zstring' modifier='reference' const='false' />
    <param name='message' type='zstring' modifier='reference' const='false' />
    <param name='source' type='zstring' modifier='reference' const='false' />
    <param name='lineNum' type='integer' modifier='value' const='false' />
    <description>Log a success message to the console.</description>
  </function>
  <function name='_format'>
    <param name='id' type='zstring' modifier='reference' const='false' />
    <param name='message' type='zstring' modifier='reference' const='false' />
    <param name='source' type='zstring' modifier='reference' const='false' />
    <param name='lineNum' type='integer' modifier='value' const='false' />
    <returns type='string' modifier='value' />
    <private />
    <description>Write all messages to a standard format.</description>
  </function>
  <namespace>Console</namespace>
</module>
